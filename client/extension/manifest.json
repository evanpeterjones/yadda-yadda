{
  "name": "YAPP Share",
  "version": "1.0",
  "description": "An extension to share a link on Yapp!",
  "permissions" : [
    "activeTab",
    "storage",
    "tabs",
    "declarativeContent",
    "geolocation"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action" : {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
